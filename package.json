{
  "name": "synara-electron",
  "version": "1.0.0",
  "description": "An Electron application with Svelte and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "preinstall": "cd src/renderer; pnpm install",
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "pnpm run typecheck:node && pnpm run svelte-check",
    "start": "electron-vite preview",
    "dev": "concurrently -p \"[{time}] [{name}]\" -n=sveltekit,electron -c=red,blue \"electron-vite dev --watch\" \"pnpm run dev:sveltekit\"",
    "build": "pnpm run build:sveltekit; npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "dev:sveltekit": "cd src/renderer; pnpm run dev",
    "build:sveltekit": "cd src/renderer; pnpm run build"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron/notarize": "^3.1.0",
    "@electron/rebuild": "^4.0.1",
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.38.0",
    "@fontsource/major-mono-display": "^5.2.8",
    "@lucide/svelte": "^0.548.0",
    "@skeletonlabs/skeleton": "^4.2.0",
    "@skeletonlabs/skeleton-svelte": "^4.2.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.48.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^22.18.6",
    "classnames": "^2.5.1",
    "concurrently": "^9.2.1",
    "electron": "github:castlabs/electron-releases#v37.2.5+wvcus",
    "electron-builder": "^25.1.8",
    "electron-serve": "^3.0.0",
    "electron-store": "^11.0.2",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "mpris-service": "^2.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "svelte": "^5.39.3",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.6",
    "x11": "^2.3.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "abstract-socket",
      "electron",
      "electron-vibrancy",
      "esbuild",
      "usocket"
    ],
    "patchedDependencies": {
      "mpris-service": "patches/mpris-service.patch"
    }
  }
}
