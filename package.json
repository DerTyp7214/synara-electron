{
  "name": "synara-desktop",
  "version": "1.0.0-1",
  "description": "Synara Desktop Music-Player",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "pnpm run typecheck:node && pnpm run svelte-check",
    "start": "electron-vite preview",
    "dev": "concurrently -p \"[{time}] [{name}]\" -n=electron,sveltekit -c=red,blue \"electron-vite dev\" \"pnpm run dev:sveltekit\"",
    "build": "pnpm run build:sveltekit && npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win --x64 --arm64",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "dev:sveltekit": "cd src/renderer && pnpm run dev",
    "sync:sveltekit": "cd src/renderer && pnpm svelte-kit sync",
    "build:sveltekit": "cd src/renderer && pnpm run build",
    "build:icons": "pnpx electron-icon-builder --input=resources/icon.png --output=build/ --flatten"
  },
  "dependencies": {
    "@electron-toolkit/preload": "catalog:",
    "@electron-toolkit/utils": "catalog:",
    "@xhayper/discord-rpc": "catalog:",
    "canvas": "catalog:",
    "electron-liquid-glass": "catalog:"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "catalog:",
    "@electron-toolkit/eslint-config-ts": "catalog:",
    "@electron-toolkit/tsconfig": "catalog:",
    "@electron/notarize": "catalog:",
    "@eslint/compat": "catalog:",
    "@eslint/js": "catalog:",
    "@types/node": "catalog:",
    "bonjour-service": "catalog:",
    "concurrently": "catalog:",
    "electron": "catalog:",
    "electron-builder": "catalog:",
    "electron-serve": "catalog:",
    "electron-store": "catalog:",
    "electron-vite": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "eslint-plugin-svelte": "catalog:",
    "file-type": "catalog:",
    "flow-bin": "catalog:",
    "globals": "catalog:",
    "mpris-service": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-svelte": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "vite": "catalog:",
    "x11": "catalog:"
  },
  "pnpm": {
    "patchedDependencies": {
      "mpris-service": "patches/mpris-service.patch"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@tailwindcss/oxide",
      "abstract-socket",
      "canvas",
      "core-js",
      "electron",
      "electron-vibrancy",
      "esbuild",
      "sharp",
      "usocket"
    ]
  }
}
